{
  "name": "vertex-claude-4-opus",
  "displayName": "Claude 4 Opus",
  "version": "0.1.2",
  "scope": "public",
  "description": "Vertex AI Claude 4 Opus Model for advanced text and image analysis using Anthropic's Claude via Google Cloud Vertex AI. Works exclusively with prompt items.",
  "attributes": {
    "Provider": "Anthropic",
    "Category": "Model",
    "Gen AI": "LLM",
    "Deployed By": "Google",
    "Media Type": [
      "Image",
      "Text"
    ]
  },
  "codebase": {
    "type": "git",
    "gitUrl": "https://github.com/dataloop-ai-apps/google-vertex-adapters.git",
    "gitTag": "0.1.2"
  },
  "components": {
    "computeConfigs": [
      {
        "name": "vertex-claude-4-opus-deploy",
        "runtime": {
          "podType": "regular-xs",
          "concurrency": 10,
          "runnerImage": "gcr.io/viewo-g/piper/agent/runner/apps/google-vertex-adapters:0.1.1",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "claude-4-opus-module",
        "entryPoint": "adapters/claude/claude.py",
        "computeConfig": "vertex-claude-4-opus-deploy",
        "className": "ModelAdapter",
        "description": "Vertex AI Claude 4 Opus Model Adapter",
        "integrations": [
          "dl-gcp-service-account"
        ],
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of prompt items to analyze with Claude 4 Opus"
              }
            ],
            "output": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "The same input prompt items for Claude analysis."
              },
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "The Claude response annotations."
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "Vertex AI Claude 4 Opus Model predict items - works exclusively with prompt items"
          }
        ]
      }
    ],
    "models": [
      {
        "name": "vertex-claude-4-opus",
        "moduleName": "claude-4-opus-module",
        "scope": "project",
        "status": "pre-trained",
        "configuration": {
          "model_id": "claude-opus-4@20250514",
          "max_tokens": 1024
        },
        "description": "Claude 4 Opus model for advanced text and image analysis through prompt-based conversations. Works exclusively with prompt items."
      }
    ]
  }
}